{
  "name": "backend",
  "version": "0.1.0",
  "main": "dist/app.js",
  "private": true,
  "homepage": "https://github.com/knowledge-technology/inloco#readme",
  "scripts": {
    "test": "cross-env NODE_ENV=tests jest --runInBand",
    "build": "tsc",
    "dev": "cross-env NODE_ENV=develop ts-node-dev --inspect --respawn src/server.ts --transpile-only --ignore-watch node_modules --respawn",
    "start": "node dist/server.js",
    "pm2:start": "cross-env NODE_ENV=production pm2 start dist/src/server.js --name Inlocco",
    "lint": "eslint . --ext .ts",
    "dup": "docker compose up -d",
    "seed": "npx prisma db seed",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:deploy": "npx prisma migrate deploy"
  },
  "author": "Matheus Barbosa <maatheusb96@gmail.com> (https://barbosamaatheus.github.io/)",
  "repository": "https://github.com/knowledge-technology/inloco.git",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "jest": "^29.4.1",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "@types/multer": "^1.4.7",
    "aws-sdk": "^2.1522.0",
    "axios": "^1.3.6",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "pino": "^8.9.0",
    "pino-http": "^8.3.3",
    "pm2": "^5.3.0",
    "prisma": "^4.13.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
